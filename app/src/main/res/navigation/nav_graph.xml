<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    tools:ignore="InvalidNavigation">
    <fragment
        android:id="@+id/mainFlowFragment"
        android:name="com.alish.geekbank.presentation.ui.fragments.main.MainFlowFragment"
        android:label="flow_fragment_main"
        tools:layout="@layout/flow_fragment_main" >
        <action
            android:id="@+id/action_mainFlowFragment_to_signFlowFragment"
            app:destination="@id/signFlowFragment" />
    </fragment>
    <fragment
        android:id="@+id/signFlowFragment"
        android:name="com.alish.geekbank.presentation.ui.fragments.sign.SignFlowFragment"
        android:label="flow_fragment_sign"
        tools:layout="@layout/flow_fragment_sign" >
        <action
            android:id="@+id/action_signFlowFragment_to_mainFlowFragment"
            app:destination="@id/mainFlowFragment" />
    </fragment>
</navigation>